{
  "private": true,
  "description": "Material Components Web",
  "license": "Apache-2.0",
  "scripts": {
    "build": "cross-env BABEL_ENV=development webpack --progress --colors",
    "build:min": "cross-env BABEL_ENV=production webpack -p --progress --colors",
    "package": "babel index.js --out-dir lib ",
    "!postinstall": "yarn build && yarn build:min && node ./scripts/cp-pkgs.js",
    "bootstrap": "yarn && lerna bootstrap",
    "clean": "del-cli packages/*/dist && lerna clean --yes",
    "dist": "yarn && yarn clean && yarn build && yarn build:min",
    "start": "yarn dist && yarn bootstrap && lerna run --scope web start"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-react-app": "^3.0.3",
    "cp-file": "^5.0.0",
    "cross-env": "^5.1.1",
    "del-cli": "^1.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "glob": "^7.1.2",
    "jtest": "^0.0.1",
    "lerna": "^2.5.1",
    "mkdirp": "^0.5.1",
    "to-slug-case": "^1.0.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  },
  "workspaces": [
    "packages/*",
    "web"
  ],
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "dependencies": {
    "babel-loader": "^7.1.2",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  }
}
